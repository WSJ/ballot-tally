{
  "parser": "esprima",
  "env": {
    "node": true
  },
  "rules": {
    "indent": [2, 2],
    "brace-style": [2, "stroustrup"],
    "comma-style": [2, "last"],
    "curly": [2, "multi-line"],
    "quotes": [2, "single"],
    "object-curly-spacing": [2, "never"],
    "strict": [2, "global"],
    "no-debugger": 2,
    "no-console": 2,
    "handle-callback-err": 2,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "no-mixed-requires": 0,
    "no-use-before-define": 0
  }
}
